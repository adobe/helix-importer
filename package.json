{
  "name": "@adobe/helix-importer",
  "version": "1.0.0",
  "description": "Helix importer tools",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha test/**/*.spec.js",
    "format": "prettier --write \"src/**/*.js\"",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.2",
    "eslint": "8.6.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "mocha": "^9.1.3",
    "mock-fs": "4.13.0",
    "prettier": "2.2.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adobe/helix-markdown-support": "2.0.1",
    "@adobe/helix-md2docx": "git+https://github.com/kptdobe/helix-md2docx.git",
    "form-data": "4.0.0",
    "fs-extra": "9.0.1",
    "hast-util-to-html": "^8.0.3",
    "hast-util-to-mdast": "^8.3.0",
    "jsdom": "16.4.0",
    "node-fetch": "2.6.1",
    "rehype-parse": "7.0.1",
    "rehype-remark": "8.0.0",
    "remark-gfm": "1.0.0",
    "remark-parse": "9.0.0",
    "remark-stringify": "8.1.1",
    "sanitize-filename": "1.6.3",
    "unified": "9.2.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
