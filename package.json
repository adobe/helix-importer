    "form-data": "4.0.0",
    "fs-extra": "11.1.1",
    "hast-util-to-mdast": "9.0.0",
    "jsdom": "22.1.0",
    "node-fetch": "3.3.1",
    "rehype-parse": "8.0.4",
    "rehype-remark": "github:adobe-rnd/rehype-remark#45735e80a3d3c805d3c4211ae035f718ddd10bcb",
    "remark-stringify": "10.0.3",
    "unified": "10.1.2"
